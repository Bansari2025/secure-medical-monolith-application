		<version>3.5.5</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.gendx.showcase</groupId>
	<artifactId>secure-medical-monolith</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>secure-medical-monolith</name>
	<description>A secure, robust, and maintainable application that serves as a single, centralized entry point for medical data.</description>
	<url/>
	<licenses>
		<license/>
	</licenses>
	<developers>
		<developer/>
